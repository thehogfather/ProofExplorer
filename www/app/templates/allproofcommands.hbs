 <div class="panel panel-primary  center-block" id="setup-group">
     <div class="panel-heading">
        <h3 class="panel-title">Select favorite proof commands</h3>
     </div>
    <div class="panel-body">
        <div  id="toolbox-palette">
            {{#each this}}
                <div title="{{label}}" class="tool-group" id="{{id}}">
                        {{#each children}}
                            <div class='tool input-group margin-bottom-sm' title='{{command}}'>
                                <span class="input-group-addon">
                                    <i class='fa {{icon}}'></i>
                                    <input type="checkbox">
                                </span>
                                <span class='tool-label form-control'>{{label}}</span>
                            </div>
                        {{/each}}
                </div>
            {{/each}}
        </div>
        <button role="button" class="btn btn-primary" id="close">Close</button>
    </div>
</div>